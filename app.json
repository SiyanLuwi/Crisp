{
  "expo": {
    "name": "Crisp",
    "slug": "Crisp",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/images/android-icon.png",
    "scheme": "crisp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash_screen.png",
      "resizeMode": "cover"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAe1iu4272Y5pQ6ccdJUYEAp6qyTAbM_-0"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.bscs.bmmn.crisp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/android-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-camera",
      "expo-asset",
      "expo-location",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c7b9d51e-3b89-4da1-a947-b1d13c069826"
      }
    }
  }
}
